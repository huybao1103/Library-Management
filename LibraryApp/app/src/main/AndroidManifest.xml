<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.librarydemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.Books.Publisher.PublisherList"
            android:exported="false" />
        <activity
            android:name=".Activity.Books.BookDetail"
            android:exported="false" />
        <activity
            android:name=".Sample.SampleActivity"
            android:exported="false" />
        <activity android:name=".Login" />
        <activity android:name=".NewPassword" />
        <activity android:name=".CodeForgotPassword" />
        <activity
            android:name=".ForgotPassword"
            android:parentActivityName=".Login" />
        <activity android:name=".UpdateBook" />
        <activity android:name=".ArrayLog" />
        <activity android:name=".ChangPass" />
        <activity android:name=".UserInformation" />
        <activity android:name=".LogInformation" />
        <activity android:name=".BookInformation" />
        <activity
            android:name=".Activity.Books.LayOutAndLisView"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SignUp" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>